{
	"name": "yespbs/egnyte",
	"description": "Manage Egnyte uploads with the Egnyte public API",
	"type": "package",
	"authors": [
        {
            "name": "Priyabrata",
            "email": "yespbs@gmail.com"
        }
    ],
	"require": {
		"php": ">=5.6.4",
		"php-curl-class/php-curl-class": "^7.0"
	},
	"require-dev": {
        "vlucas/phpdotenv": "^2.4@dev",
        "phpunit/phpunit": "^6.0"
    },
	"license": "MIT",
	"autoload": {
        "psr-4": {
            "Yespbs\\Egnyte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yespbs\\Egnyte\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
	},
	"config": {
        "sort-packages": true
    }
}
